#include <iostream>

using namespace std;

int RangeCalc()
{
    float sizeInInches;
    float sizeInYards;
    float sizeInMils;
    float RangeInYards;
    float altitudeDifference;

    cout<<"What is the size of the reference in inches?"<<endl;
    cin>>sizeInInches;

    sizeInYards = sizeInInches / 36;

    cout<<"What is the size of the reference in mils"<<endl;
    cin>>sizeInMils;
    
    cout<<"What is the difference in altitude between you and your target"<<endl;
    cin>>altitudeDifference;
    

    cout<<sizeInYards*1000/sizeInMils+altitudeDifference;


    return 0;
}
